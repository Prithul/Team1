<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Employee REst API Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="71.0.3578.98"/>
   <!-- <parameter name="url" value="https://www.cnn.com"/> -->
    <parameter name="user" value="restAPI"/>
    <test name = "RestAPI Test">
            <classes>
            <class name="rest.SingleEmployeeAPI"/>
            <class name="rest.AllEmployeeAPI"/>
        </classes>
    </test>

</suite>